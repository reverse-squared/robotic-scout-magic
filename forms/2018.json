{
    "$schema": "../form-schema.json",
    "icon": "arrow-up",
    "id": "2018",
    "name": "2018 FIRST Power Up",
    "hidden": true,
    "description": "A scouting form for 2018's FIRST Power Up Game",
    "items": [
        {
            "type": "number",
            "label": "The Team / Robot Number",
            "exportLabel": "Team Number"
        },
        {
            "type": "header",
            "label": "Autonomous Period"
        },
        {
            "type": "boolean",
            "label": "Did the robot cross the auto line?",
            "trueValue": "Yes",
            "falseValue": "No",
            "default": false
        },
        {
            "type": "counter",
            "label": "How many cubes were scored on the scale during the autonomous period?",
            "exportLabel": "Scale Cubes Scored in Auton"
        },
        {
            "type": "counter",
            "label": "How many cubes were scored on the switch during the autonomous period?",
            "exportLabel": "Switc Cubes Scored in Auton"
        },
        {
            "type": "header",
            "label": "Teleop Period"
        },
        {
            "type": "counter",
            "label": "How many cubes were scored on the scale?",
            "exportLabel": "Scale Cubes Scored"
        },
        {
            "type": "counter",
            "label": "How many cubes were scored on the switch?",
            "exportLabel": "Switch Cubes Scored"
        },
        {
            "type": "counter",
            "label": "How many cubes were scored in the exchange?",
            "exportLabel": "Exchange Cubes Scored"
        },
        {
            "type": "header",
            "label": "End Game"
        },
        {
            "type": "radio",
            "label": "What did the robot do at the end of the match?",
            "options": [
                "The robot climbed.",
                "The robot ended on the mat.",
                "The robot didn't climb or end on the mat."
            ],
            "exportLabel": "Where Was the Robot At the End?"
        },
        {
            "type": "text-area",
            "label": "Other notes.",
            "exportLabel": "Notes"
        }
    ]
}
